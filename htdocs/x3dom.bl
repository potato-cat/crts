#!/bin/bash

set -ex

rm -rf x3dom

tar -xvzf x3dom.tar.gz
mv vtwireless-x3dom-1.5.0-dev-* x3dom
touch x3dom


exit 0
# Below is what we would do if we got the latest version of
# x3dom, that came as a zip file, and it supported
# elevationGrid; it does not so we use a different older
# version of it.

sha512sum -c x3dom.zip.sha512sum
rm -rf x3dom


unzip x3dom.zip -d x3dom
